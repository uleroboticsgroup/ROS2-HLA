                <attribute>
                    <name>value</name>
                    <dataType>HLAunicodeString</dataType>
                    <updateType>Static</updateType>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                </attribute>
            </objectClass>
            <objectClass>
                <name>ListenerStatus</name>
                <sharing>PublishSubscribe</sharing>
                <attribute>
                    <name>ready</name>
                    <dataType>HLAunicodeString</dataType>
                    <updateType>Static</updateType>
                    <transportation>HLAreliable</transportation>
                    <order>Receive</order>
                </attribute>
            </objectClass>
        </objectClass>
    </objects>
